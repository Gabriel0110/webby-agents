{
  "name": "webby-agent",
  "version": "0.1.0",
  "description": "A small, simple, and modular TypeScript-powered agent framework designed for seamless web application integration. Simplify AI-powered tools, web searches, and automation with Webby-Agent.",
  "keywords": [
    "agents",
    "typescript",
    "web applications",
    "AI",
    "OpenAI",
    "framework",
    "tool integration"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Gabriel0110/webby-agent.git"
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --project tsconfig.json --module CommonJS --outDir dist/cjs",
    "build:esm": "tsc --project tsconfig.json --module ESNext --outDir dist/esm",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "axios": "^1.2.0",
    "duck-duck-scrape": "^2.2.6",
    "openai": "^3.2.1"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.30",
    "@types/node": "^18.11.9",
    "cheerio": "^1.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  }
}
